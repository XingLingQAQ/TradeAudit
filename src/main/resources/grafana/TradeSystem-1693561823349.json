          "rawQuery": true,
          "rawSql": "SELECT \r\n    type,\r\n    SUM(\r\n        CASE \r\n            WHEN type = 'Exp level icon' THEN\r\n                CASE \r\n                    WHEN quantity < 353 THEN SQRT(quantity + 9) - 3\r\n                    WHEN quantity < 1508 THEN 8.1 + SQRT(0.4 * (quantity - 195.975))\r\n                    ELSE 325 / 18 + SQRT(2 / 9 * (quantity - 54215 / 72))\r\n                END\r\n            ELSE quantity\r\n        END\r\n    ) AS amount\r\nFROM ${database}.trade_metrics AS m\r\nLEFT JOIN ${database}.trade_players AS p ON m.sender = p.id\r\nWHERE $__timeFilter(date) AND category LIKE 'economy' AND (LENGTH(\"$playerFilter\") = 2 OR name REGEXP \"${playerFilter:regex}\")\r\nGROUP BY type;\r\n",
