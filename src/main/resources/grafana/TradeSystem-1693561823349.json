          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT name as player, uuid, type, \r\n    IF(${playereconomyfilter} LIKE \"#included in trades\", \r\n        COUNT(*), \r\n        SUM(\r\n            CASE \r\n                WHEN type = 'Exp level icon' THEN\r\n                    CASE \r\n                        WHEN quantity < 353 THEN SQRT(quantity + 9) - 3\r\n                        WHEN quantity < 1508 THEN 8.1 + SQRT(0.4 * (quantity - 195.975))\r\n                        ELSE 325 / 18 + SQRT(2 / 9 * (quantity - 54215 / 72))\r\n                    END\r\n                ELSE quantity\r\n            END\r\n        )\r\n    ) as ${playereconomyfilter},\r\n    CASE \r\n        WHEN ${playereconomyfilter} LIKE \"Exp level icon\" THEN\r\n            \"#levels traded\"\r\n        WHEN ${playereconomyfilter} LIKE \"#included in trades\" THEN\r\n            \"The number of trades that include offers with this economy type.\"\r\n        ELSE \"-\"\r\n    END as \"description\"\r\nFROM ${database}.trade_metrics as m \r\nLEFT JOIN ${database}.trade_players as p ON m.sender = p.id \r\nWHERE $__timeFilter(date) AND category LIKE \"economy\" AND (LENGTH(\"$playerFilter\") = 2 OR name REGEXP \"${playerFilter:regex}\")\r\nGROUP BY sender, type\r\nORDER BY SUM(quantity) DESC\r\nLIMIT 15",
