          "rawSql": "SELECT\r\n    AVG(TIME_TO_SEC(TIMEDIFF(\r\n        t.timestamp, \r\n        (\r\n            SELECT timestamp \r\n            FROM ${database}.tradelog \r\n            WHERE timestamp <= t.timestamp AND t.player1 = player1 AND t.player2 = player2 AND message LIKE \"Trade started\" \r\n            ORDER BY timestamp DESC \r\n            LIMIT 1\r\n        )\r\n    ))) as duration\r\nFROM ${database}.tradelog as t\r\nWHERE $__timeFilter(t.timestamp) AND message IN (\"Trade cancelled\", \"Trade finished\") AND (LENGTH(\"$playerFilter\") = 2 OR player1 REGEXP \"${playerFilter:regex}\" OR player2 REGEXP \"${playerFilter:regex}\")",
